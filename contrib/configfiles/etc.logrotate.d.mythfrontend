/var/log/mythtv/mythfrontend.log {
    compress
    compresscmd /usr/bin/bzip2
    uncompresscmd /usr/bin/bunzip2
    compressext .bz2
    delaycompress
    missingok
    notifempty
    postrotate
        /bin/kill -HUP `pidof mythfrontend 2> /dev/null` 2> /dev/null || true
    endscript
    rotate 8
    weekly
}
